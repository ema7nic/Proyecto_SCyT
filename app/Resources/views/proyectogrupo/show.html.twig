{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proyectogrupo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proyectoGrupo.id }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ proyectoGrupo.codigo }}</td>
            </tr>
            <tr>
                <th>Codigoutn</th>
                <td>{{ proyectoGrupo.codigoUtn }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ proyectoGrupo.nombre }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if proyectoGrupo.fechaInicio %}{{ proyectoGrupo.fechaInicio|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{% if proyectoGrupo.fechaFin %}{{ proyectoGrupo.fechaFin|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Saldo</th>
                <td>{{ proyectoGrupo.saldo }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proyectogrupo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('proyectogrupo_edit', { 'id': proyectoGrupo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
