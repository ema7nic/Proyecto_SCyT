{% extends 'base.html.twig' %}

{% block body %}
    <h1>Asignación</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ asignacion.id }}</td>
            </tr>
           
            <tr>
                <th>Monto</th>
                <td>{{ asignacion.monto }}</td>
            </tr>
           
            <tr>
                <th>Fecha</th>
                <td>{% if asignacion.fechaAsignacion %}{{ asignacion.fechaAsignacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ asignacion.descripcion }}</td>
            </tr>
        </tbody>
    </table>

    
{% endblock %}
